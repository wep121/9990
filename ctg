        }
        // 循环组件列表，把组件的moduleID赋值给searchUiPc相应组件的key。
        componentList.forEach(function (item, index) {
            var key = item.split('_')[0];
            key = key.match(/^search-ui-pc\/(.+)/)[1].replace(/\//g, '_');
            searchUiPc[key] = item;
        });
